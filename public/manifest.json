{
  "manifest_version": 2,

  "name": "OnyxPay web wallet",
  "author": "OnyxPay <mail@onyxpay.co>",
  "description": "OnyxPay web wallet",
  "version": "0.1.6",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "32": "favicon-32x32.png",
    "48": "favicon.icon",
    "128": "logo.png"
  },
  "permissions": [
    "storage"
  ]
}